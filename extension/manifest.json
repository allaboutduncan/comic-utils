{
  "manifest_version": 3,
  "name": "Send Link to CLU",
  "version": "1.0",
  "description": "Right-click a link to send it to your CLU downloads.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  }
}
